{
  "collectionName": "components_module_pleine_pages",
  "info": {
    "displayName": "Pleine page",
    "description": "",
    "icon": "picture"
  },
  "options": {},
  "attributes": {
    "medias": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images", "videos"]
    },
    "gap": {
      "type": "integer",
      "min": 0,
      "default": 0,
      "required": true
    },
    "backgroundImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "backgroundColor": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "text": {
      "type": "component",
      "repeatable": false,
      "component": "module.text-only"
    }
  }
}
