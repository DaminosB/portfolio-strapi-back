{
  "kind": "singleType",
  "collectionName": "logos",
  "info": {
    "singularName": "logo",
    "pluralName": "logos",
    "displayName": "Logos",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "moduleSelection": {
      "type": "dynamiczone",
      "components": [
        "module.container",
        "module.pleine-page",
        "module.colonne-multi-images"
      ]
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "isVisible": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "thumbnailColor": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    }
  }
}
